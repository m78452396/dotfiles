{
    "archinstall-language": "English",
    "audio_config": null,
    "bootloader": "Limine",
    "custom_commands": [
        "systemctl enable greetd.service NetworkManager ModemManager.service paccache.timer bluetooth.service",
        "mkdir -p /home/arch/.scripts/",
        "mkdir -p /home/arch/.config/",
        "mkdir -p /home/arch/.config/htop",
        "mkdir -p /home/arch/.config/dunst",
        "mkdir -p /home/arch/.config/labwc",
        "mkdir -p /home/arch/.config/alacritty",
        "wget -c github.com/rustdesk/rustdesk/releases/download/1.4.0/rustdesk-1.4.0-0-x86_64.pkg.tar.zst && pacman -U --noconfirm rustdesk-1.4.0-0-x86_64.pkg.tar.zst && rm rustdesk-1.4.0-0-x86_64.pkg.tar.zst",
        "wget -P /home/arch/.scripts/ -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/randr-m.sh",
        "wget -P /home/arch/.scripts/ -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/randr-n.sh",
        "wget -P /home/arch/.scripts/ -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/ffplay.sh",
        "wget -P /home/arch/.config/htop -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/htoprc",
        "wget -P /home/arch/.config/dunst -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/dunstrc",
        "wget -P /home/arch/.config/labwc -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/autostart",
        "wget -P /home/arch/.config/labwc -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/environment",
        "wget -P /home/arch/.config/labwc -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/menu.xml",
        "wget -P /home/arch/.config/labwc -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/rc.xml",
        "wget -P /home/arch/.config/alacritty -c raw.githubusercontent.com/m78452396/dotfiles/refs/heads/main/alacritty.toml",
        "chmod +x /home/arch/.scripts/*.sh",
        "chown -R arch:arch /home/arch/.scripts",
        "chown -R arch:arch /home/arch/.config"
    ],
    "hostname": "archlinux",
    "kernels": [
        "linux"
    ],
    "locale_config": {
        "kb_layout": "br-abnt2",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US.UTF-8"
    },
    "ntp": true,
    "packages": [
        "alacritty",
        "bash-completion",
        "bluez",
        "bluez-utils",
        "brightnessctl",
        "chromium",
        "dunst",
        "ffmpeg",
        "greetd",
        "grim",
        "gvfs",
        "htop",
        "intel-media-driver",
        "labwc",
        "libnotify",
        "modemmanager",
        "nano",
        "networkmanager",
        "noto-fonts",
        "noto-fonts-cjk",
        "noto-fonts-emoji",
        "noto-fonts-extra",
        "pacman-contrib",
        "pipewire-audio",
        "rsync",
        "slurp",
        "sshfs",
        "upower",
        "usb_modeswitch",
        "vulkan-intel",
        "wget",
        "wireless-regdb",
        "wireplumber",
        "wl-clipboard",
        "wlr-randr",
        "xdg-desktop-portal-wlr",
        "xdg-user-dirs-gtk"
    ],
    "parallel_downloads": 0,
    "services": [],
    "swap": true,
    "timezone": "America/Sao_Paulo",
    "version": null
}
